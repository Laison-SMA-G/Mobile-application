{
  "cli": {
    "version": ">= 16.27.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "android": { "buildType": "apk" },
      "distribution": "internal",
      "env": {
        "BASE_URL": "http://192.168.100.45:5000/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "BASE_URL": "https://mobile-application-2.onrender.com/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": { "buildType": "app-bundle" },
      "env": {
        "BASE_URL": "https://mobile-application-2.onrender.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
